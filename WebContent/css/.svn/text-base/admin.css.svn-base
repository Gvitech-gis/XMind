/*-----global全局样式-----*/
html, body, div, p, ul, li, dl, dt, dd, h1, h2, h3, h4, h5, h6, form, input, select, button, textarea, iframe, table, th, td {margin:0; padding:0;font-family:"宋体",arial;font-size:12px;}
body {font-size:12px;background: url(../images/main_bg.jpg) #ffffff  top center scroll repeat-y ; font-family:"宋体", Arial, Helvetica, sans-serif; color:#333333;  min-heigth:min-height:100%; height:auto !important; height:100%; line-height:30px; }

html {  
  height:100%;  
}  


ul,li,p{list-style-type: none; }
img{border:0 none; margin: 0; padding: 0;}
h1{ font-size:16px; font-weight:bold;}
h2{ font-size:14px; color: #666666;}
h3{ font-size:12px;}
a:link {text-decoration:none;color: #333333;}
a:hover {text-decoration:none;color:#333333;}
a:active {text-decoration: underline;color: #333333;}
a:visited {text-decoration: none;color: #333333;}
.clear{clear:both; display:inline; font-size:1px;height:0;line-height:0;}
.clear1{height:1px;line-height:1px;font-size:1px;clear:both;margin-top:-1px;}




.left{float:left;}
.right{float:right;}
.center{margin:0 auto;}
.bold{font-weight:bold;}
.red, a.red{color:#C00;}
.redbold{color:#F00; font-weight:bold;}
.white,a.white,.white a:hover,.white a:link,.white a:visited{color:#ffffff;}
.none{display:none;}
.blue{color: #1985ca;}
.select{border:1px solid #67a3d7; width:60px; }
.select_time{border:1px solid #67a3d7; width:40px; }
