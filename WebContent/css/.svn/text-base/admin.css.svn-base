/*5.左侧导航样式*/
#left-nav	{color:#fff; text-decoration:none; border-bottom:1px solid #ededed; text-indent:15px;} 
#left-nav a	{color:#fff; text-decoration:none; background:#278DCD;} 
#left-nav a:hover	{text-decoration:underline; color:#333; background:#eee;} 
#left-nav a.active	{text-decoration:none; height:26px; color:#006ebd; background:url(../images/left_overgif.gif) no-repeat; display:block; font-size:14px; font-weight:bold;} 
.top-level a, .second-level a, .third-level a	{display:block; margin:0; padding:2px 0 2px 0; border-top:1px solid #fff;  } 
.top-level a	{padding-left:12px;} 
.second-level a	{padding-left:24px;} 
.third-level a	{padding-left:36px;} 
.top-level a	{color:#000; background:#9cf;} 
.top-level a#site-home	{font-size:1.1em;} 
.second-level a, .third-level a, a.open	{color:#000; background:#cce5ff;} 
a.active	{color:#000; background:#fff;} 


